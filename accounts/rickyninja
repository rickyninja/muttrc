set from = $my_rickyninja_email
set imap_user = $my_rickyninja_email
set imap_pass = $my_rickyninja_pass
set smtp_url = "smtp://$my_rickyninja_email@smtp.gmail.com:587/"
set smtp_pass = $my_rickyninja_pass
unset folder
set folder = "imaps://$my_rickyninja_email@imap.gmail.com:993"
source ~/.mutt/gmail-imap
